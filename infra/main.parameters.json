{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environmentName": {
      "value": "${AZURE_ENV_NAME}"
    },
    "location": {
      "value": "${AZURE_LOCATION}"
    },
    "openAiLocation": {
      "value": "${AZURE_OPENAI_LOCATION=eastus2}"
    },
    "principalId": {
      "value": "${AZURE_PRINCIPAL_ID}"
    },
    "principalType": {
      "value": "${AZURE_PRINCIPAL_TYPE=User}"
    },
    "restoreSoftDeletedOpenAi": {
      "value": "${restoreSoftDeletedOpenAi=false}"
    },
    "useAFD": {
      "value": "${USE_AFD=true}"
    },
    "useAPIM": {
      "value": "${USE_APIM=true}"
    },
    "apimSku": {
      "value": "${APIM_SKU=BasicV2}"
    },
    "backendImageName": {
      "value": "${SERVICE_BACKEND_IMAGE_NAME=}"
    },
    "useAgents": {
      "value": "${USE_AGENTS=false}"
    },
    "agentImageName": {
      "value": "${SERVICE_AGENT_IMAGE_NAME=}"
    }
  }
}
